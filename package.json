{
  "name": "@genisense/dlog",
  "version": "0.1.7",
  "description": "Development time logging with dynmaic type checking.",
  "main": "dist/dlog.js",
  "repository": "git@github.com:logworks/dlog.git",
  "bin": {
    "dlog": "src/cli/dlog.js"
  },
  "preferGlobal": false,
  "scripts": {
    "build": "webpack  && cp dist/dlog.js examples/web/",
    "test:app": "jest src/app",
    "test:cli": "jest src/cli",
    "coverage": "./node_modules/.bin/nyc report",
    "test": "npm run test:cli && npm run test:app",
    "tdd": "jest --watch --coverage",
    "commit": "git-cz",
    "devlog": "node ./src/cli/dlog.js"
  },
  "keywords": [],
  "author": "Henry Kemp",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "cz-conventional-changelog": "^3.1.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "async": "^3.1.1",
    "coveralls": "^3.0.9",
    "deep-diff": "^1.0.2",
    "gift": "^0.10.2",
    "glob": "^7.1.6"
  },
  "jest": {
    "verbose": false,
    "rootDir": "src",
    "transform": {}
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
