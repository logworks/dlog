{
  "name": "@genisense/dlog",
  "version": "0.2.67",
  "description": "Development time logging with dynmaic type checking.",
  "main": "dist/dlog.js",
  "repository": "git@github.com:logworks/dlog.git",
  "bin": {
    "dlog": "src/cli/dlog.js"
  },
  "preferGlobal": false,
  "scripts": {
    "build": "webpack  -p --devtool source-map && cp dist/dlog.js examples/2-web-simplest/ ",
    "lint": "./node_modules/.bin/eslint src",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "test": "jest",
    "tdd": "jest --watch",
    "commit": "git-cz",
    "dlog": "node ./src/cli/dlog.js",
    "hack": "cd examples/6-packaged-dlogger-privrepo/acme-dlogger-node/ && npm update @genisense/dlog",
    "example:node": "cd examples/1-node-simplest",
    "example:web": "npm run build && cd examples/2-web-simplest && http-server"
  },
  "keywords": [],
  "author": "Henry Kemp",
  "license": "ISC",
  "devDependencies": {
    "@genisense/dlog": "^0.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "prettier": "^1.19.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "async": "^3.1.1",
    "deep-diff": "^1.0.2",
    "error-stack-parser": "^2.0.6",
    "gift": "^0.10.2",
    "glob": "^7.1.6",
    "ms": "^2.1.2"
  },
  "jest": {
    "verbose": false,
    "rootDir": "src",
    "transform": {}
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm build && npm run lint && npm test",
      "post-commit": "git push --set-upstream dlog-p working:master && npm version patch --force && npm run hack"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
